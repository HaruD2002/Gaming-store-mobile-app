<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/while2">


    <ImageButton
        android:id="@+id/btnNotifications"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_notifications_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="172dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="89dp"
        android:layout_height="84dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp"
        tools:srcCompat="@tools:sample/avatars"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="176dp"
        android:layout_height="84dp"
        android:layout_marginTop="10dp"
        android:text="Enter Name"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="125dp" />


    <ImageButton
        android:id="@+id/btnHistory"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_history_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="237dp" />

    <ImageButton
        android:id="@+id/btnPayment"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_payment_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="301dp" />

    <ImageButton
        android:id="@+id/btnecurity"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_security_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="366dp" />

    <ImageButton
        android:id="@+id/btnHelp"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_help_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="431dp" />

    <ImageButton
        android:id="@+id/btnLogout"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_logout_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="496dp" />

    <ImageButton
        android:id="@+id/btnManager"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:src="@drawable/baseline_manage_accounts_24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="110dp" />

    <TextView
        android:id="@+id/Manager"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="90dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="My Acount "
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="108dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/Notifications"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="Notifications"
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Manager" />

    <TextView
        android:id="@+id/History"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="History"
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Notifications" />

    <TextView
        android:id="@+id/Payment"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="Payment"
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/History" />

    <TextView
        android:id="@+id/Security"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="Security"
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Payment" />

    <TextView
        android:id="@+id/Help"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="Help "
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Security" />

    <TextView
        android:id="@+id/Logout"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/boder"
        android:gravity="start|center_vertical"
        android:hint="Logout "
        android:padding="16dp"
        android:textColor="@color/lavender"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Help" />

    <Button
        android:id="@+id/btnUpdate"
        android:layout_width="81dp"
        android:layout_height="64dp"
        android:text="Up date"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="16dp"
        android:backgroundTint="@color/blue1"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>